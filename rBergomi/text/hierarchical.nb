 RowBox[{"(*", 
  RowBox[{
   RowBox[{"W", "[", 
    RowBox[{"m_", ",", "t_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     SubscriptBox["Z", "0"], " ", "t"}], " ", "+", " ", 
    RowBox[{"Sum", "[", 
     RowBox[{
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Z", "[", 
          RowBox[{"n", ",", "j"}], "]"}], " ", 
         RowBox[{"F", "[", 
          RowBox[{"n", ",", "j", ",", "t"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", 
          RowBox[{
           RowBox[{"2", "^", "n"}], "-", "1"}]}], "}"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"n", ",", "1", ",", "m"}], "}"}]}], "]"}]}]}], "*)"}]], "Input",\

