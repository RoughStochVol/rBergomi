Cell["Define the exact formula of the differences at scale m.", "Text",
 CellChangeTimes->{{3.718614772538354*^9, 
  3.718614786858033*^9}},ExpressionUUID->"84b3704f-8527-409d-9d56-\
38c4ada9b38e"],

Cell[BoxData[
 RowBox[{
  RowBox[{"\[Iota]", "[", 
   RowBox[{"i_", ",", "n_", ",", "m_"}], "]"}], ":=", 
  RowBox[{"Boole", "[", 
   RowBox[{"EvenQ", "[", 
    RowBox[{"Floor", "[", 
     RowBox[{"i", " ", 
      RowBox[{"2", "^", 
       RowBox[{"(", 
        RowBox[{"n", "-", "m"}], ")"}]}]}], "]"}], "]"}], "]"}]}]], "Input",
 CellChangeTimes->{{3.718614843929738*^9, 
  3.718614935237599*^9}},ExpressionUUID->"85a9dbb7-c69e-48d8-aa02-\
0a7af7770a01"],

Cell[BoxData[
 RowBox[{
  RowBox[{"dWexact", "[", 
   RowBox[{"m_", ",", "i_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    SubscriptBox["Z", "0"], " ", 
    RowBox[{"2", "^", 
     RowBox[{"(", 
      RowBox[{"-", "m"}], ")"}]}]}], "+", 
   RowBox[{"Sum", "[", 
