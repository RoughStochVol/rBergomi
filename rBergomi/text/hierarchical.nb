Cell[CellGroupData[{
Cell["Hierarchical representation of Brownian increments", "Subsection",
 CellChangeTimes->{{3.718536137755582*^9, 
  3.718536153128477*^9}},ExpressionUUID->"e81fd5e6-e3fc-4aeb-a0d1-\
dea29b786fe6"],

Cell[TextData[{
 "Given an interval ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"0", ",", "1"}], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b9149724-d7d1-4e94-b6de-184a1a17a776"],
 ", we are trying to represent increments ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["W", 
      SubscriptBox["t", 
       RowBox[{"i", "+", "1"}]]], "-", 
     SubscriptBox["W", 
      SubscriptBox["t", "i"]]}], ",", " ", 
    RowBox[{
     SubscriptBox["t", "i"], "=", 
     RowBox[{"i", " ", 
      SuperscriptBox["2", 
       RowBox[{"-", "m"}]]}]}], ",", " ", 
    RowBox[{"i", "=", "0"}], ",", "...", ",", 
    SuperscriptBox["2", "m"], ","}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d56ef6df-c39b-44e9-8736-f5ab7d6d8be7"],
 " by means of the hierarchical representation in terms of Haar basis. We \
start with defining the Schauder functions on ",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{"0", ",", "1"}], "]"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "4cb30d80-9f7f-4204-a17e-1a364f38ad71"],
 "."
}], "Text",
 CellChangeTimes->{{3.718536159027493*^9, 3.718536174660698*^9}, {
  3.718536293046172*^9, 
  3.718536480293407*^9}},ExpressionUUID->"3ebebe79-4633-4ece-974a-\
868c28b93176"],

Cell[CellGroupData[{

Cell["Schauder functions", "Subsubsection",
 CellChangeTimes->{{3.7185364854289427`*^9, 
  3.718536491878717*^9}},ExpressionUUID->"a5204a25-b35b-4978-a700-\
833638ce17c6"],

Cell[TextData[{
 "Functions are defined for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[GreaterEqual]", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "17138137-a80f-4c3a-9617-a31711cedd1b"],
 "and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[LessEqual]", "j", "\[LessEqual]", 
    RowBox[{
     SuperscriptBox["2", "n"], "-", "1."}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b3d0d4a1-4c42-424f-85a3-6959e955256e"]
}], "Text",
 CellChangeTimes->{{3.718537064735572*^9, 3.718537101848851*^9}, {
  3.718537161582015*^9, 3.71853716490868*^9}, {3.7185372607174387`*^9, 
  3.718537262766281*^9}},ExpressionUUID->"dc765168-472a-4c3f-bd03-\
230902aef2b3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"F", "[", 
   RowBox[{"n_", ",", "j_", ",", "t_"}], "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"2", "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], "/", "2"}], ")"}]}], " ", 
         RowBox[{"(", 
          RowBox[{"t", "-", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"j", "-", "1"}], ")"}], 
            RowBox[{"2", "^", 
             RowBox[{"(", 
              RowBox[{"-", "n"}], ")"}]}]}]}], ")"}]}], ",", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"j", "-", "1"}], ")"}], 
          RowBox[{"2", "^", 
           RowBox[{"(", 
            RowBox[{"-", "n"}], ")"}]}]}], "\[LessEqual]", "t", 
         "\[LessEqual]", 
         RowBox[{"j", " ", 
          RowBox[{"2", "^", 
           RowBox[{"(", 
            RowBox[{"-", "n"}], ")"}]}]}]}]}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{
         RowBox[{"2", "^", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"n", "-", "1"}], ")"}], "/", "2"}], ")"}]}], " ", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"j", "+", "1"}], ")"}], 
            RowBox[{"2", "^", 
             RowBox[{"(", 
              RowBox[{"-", "n"}], ")"}]}]}], "-", "t"}], ")"}]}], ",", 
        RowBox[{
         RowBox[{"j", " ", 
          RowBox[{"2", "^", 
           RowBox[{"(", 
            RowBox[{"-", "n"}], ")"}]}]}], "<", "t", "\[LessEqual]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"j", "+", "1"}], ")"}], 
          RowBox[{"2", "^", 
           RowBox[{"(", 
            RowBox[{"-", "n"}], ")"}]}]}]}]}], "}"}]}], "}"}], ",", "0"}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.718536581238592*^9, 3.7185366032748127`*^9}, {
